{
  "name": "cypress-automation",
  "version": "1.0.0",
  "description": "cypress e2e and smoke test scripts",
  "main": "index.js",
  "scripts": {
    "test": "node_modules\\.bin\\cypress run",
    "headTest": "npm run test -- --headed",
    "chromeTest": "npm run test -- --browser chrome",
    "smokeTest": "npm run test -- --spec cypress/integration/Smoke/**/*",
    "acceptanceTest": "npm run test -- --spec cypress/integration/Acceptance/**/*",
    "FunctionalTest": "npm run test -- --spec cypress/integration/Functional/**/*"
   "cy:run": "npx cypress run",
   "cy:verify":"npx cypress verify",
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Akhil0907/testrepo.git"
  },
  {
  "plugins": [
    "@semantic-release/commit-analyzer",
    "@semantic-release/release-notes-generator",
    "@semantic-release/npm",
    "@semantic-release/git"
  ]
},
  "keywords": [
    "cypress"
  ],
  "author": "akhil",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Akhil0907/testrepo/issues"
  },
  "homepage": "https://github.com/Akhil0907/testrepo#readme"
}
